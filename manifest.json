{
    "name": "Github user & repo informations on news feed",
    "description": "Show user and repo informations on news feed",
    "manifest_version": 2,
    "version": "0.4",
    "permissions": [ "storage"],
    "content_scripts": [{
        "js": [ "jquery-1.9.1.min.js", "contentscript.js" ],
        "css": [ "style.css" ],
        "matches": [ "https://github.com/" ]
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "options_page": "options.html",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    }
}
